# iptables-control completion

[ -z "$(type iptables-control 2>/dev/null)" -a -z "$(type ip6tables-control 2>/dev/null)" ] && return
_iptables_control()
{
  local cur

  COMPREPLY=()
  cur=`_get_cword`

  _expand || return 0

  if [ $COMP_CWORD -eq 1 ]; then
    COMPREPLY=( $( compgen -W 'start stop restart status save' -- "$cur" ) )
  fi
  return 0
} &&
complete -F _iptables_control iptables-control
complete -F _iptables_control ip6tables-control

