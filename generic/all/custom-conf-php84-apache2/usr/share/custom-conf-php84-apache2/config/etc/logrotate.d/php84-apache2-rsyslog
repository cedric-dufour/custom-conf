## CUSTOM-CONF: File automatically created/updated [custom-conf-php84-apache2]

# Rotate rsyslog-redirected PHP 8.4 logs
/var/log/php8.4-apache2.rsyslog {
	weekly
	missingok
	rotate 10
	compress
	delaycompress
	notifempty
	create 640 %{CUSTOM_CONF_PHP84_APACHE2_LOGOWNER} adm
	sharedscripts
	postrotate
		/usr/lib/rsyslog/rsyslog-rotate
	endscript
}
